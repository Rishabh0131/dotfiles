{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "blue": "#8AADF4",
    "wight": "#FFFFFF",
    "text": "#494D64"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:wight",
          "powerline_symbol": "",
          "leading_diamond": "",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "",
          "style": "diamond",
          "template": "{{ .UserName }}",
          "type": "session"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "....",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "type": "python",
          "style": "powerline",
          "background": "p:lavender",
          "foreground": "p:text",
          "powerline_symbol": "",
          "template": "  venv "
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": " ",
            "cherry_pick_icon": " ",
            "commit_icon": " ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": " ",
            "no_commits_icon": " ",
            "rebase_icon": " ",
            "revert_icon": " ",
            "tag_icon": " "
          },
          "powerline_symbol": "",
          "template": " {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
